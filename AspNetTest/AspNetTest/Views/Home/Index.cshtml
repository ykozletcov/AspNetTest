@model AspNetTest.Models.HomeViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div>Список пакетов</div>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Login</th>
            <th>Pasword</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Users.Count > 0)
        {
            @foreach (var user in Model.Users)
             {
                 <tr>
                     <td>@Html.DisplayFor(p => user.ID)</td>
                     <td>@Html.DisplayFor(p => user.Name)</td>
                     <td>@Html.DisplayFor(p => user.Login)</td>
                     <td>@Html.DisplayFor(p => user.Password)</td>
                 </tr>
             }
        }

    </tbody>
</table>
